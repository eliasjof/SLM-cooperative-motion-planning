  doi={10.1109/JSYST.2023.3326991},
  ISSN={1937-9234},
  month={Dec},}@ARTICLE{7728117,
  author={Li, Jianqiang and Deng, Genqiang and Luo, Chengwen and Lin, Qiuzhen and Yan, Qiao and Ming, Zhong},
  journal={IEEE Transactions on Vehicular Technology}, 
  title={A Hybrid Path Planning Method in Unmanned Air/Ground Vehicle (UAV/UGV) Cooperative Systems}, 
